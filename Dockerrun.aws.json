{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image":"kokolocco/multicont_client",
			"hostname": "client",
			"essential": false,
			"memory": 128
		},
		{
			"name": "server",
			"image":"kokolocco/multicont_server",
			"hostname": "api",
			"essential": false,
			"memory": 128
		},
		{
			"name": "worker",
			"image":"kokolocco/multicont_worker",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image":"kokolocco/multicont_nginx",
			"hostname": "nginx",
			"essential": true,
			"memory": 128,
			"portMappings":[
				{
				"hostPort": 80,
				"containerPort": 80
				}
			],
			"links": ["client","server"]
		}

	]
}
